<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="launch-local" default="run-latency-benchmarks" basedir=".">
	<taskdef resource="net/sf/antcontrib/antlib.xml">
		<classpath>
			<pathelement location="lib/ant-contrib-1.0b3.jar" />
		</classpath>
	</taskdef>
    <import file="build.xml"/>
	
	<target name="run-latency-benchmarks" depends="dist">
		<ant dir="target/dist" antfile="ping-suite.xml" />
	</target>

</project>
